package Intermediario;

public class ProgramadorJava extends Programador{

 //Construtor
    public ProgramadorJava(Senioridade senioridade, String nome, int anosDeExperiencia, int idade) {
        super(senioridade, nome, anosDeExperiencia, idade);
    }

    @Override
    public void dizendoSeEstaEmpregado() {
        System.out.println("Estou empregado");
    }

    @Override
    public void dizendoLinguagem() {
        System.out.println("Meu nome Ã© " + getNome() + " Estou usando JAVA como linguagem " + "e atualmente sou de " +
                "senioridade " + getSenioridade());
    }


}
